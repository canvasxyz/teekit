{
	"name": "@teekit/images",
	"version": "0.0.5",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "scripts/check_perms.sh && scripts/setup_deps.sh && scripts/prep_kettle.sh",
		"build:vm": "bash scripts/mkosi_build",
		"build:vm:clean": "bash scripts/mkosi_build --clean",
		"build:devtools": "bash scripts/mkosi_build devtools",
		"build:devtools:clean": "bash scripts/mkosi_build --clean devtools",
		"build:gcp": "bash scripts/mkosi_build gcp",
		"build:gcp:clean": "bash scripts/mkosi_build --clean gcp",
		"build:gcp:devtools": "bash scripts/mkosi_build gcp-devtools",
		"build:gcp:devtools:clean": "bash scripts/mkosi_build --clean gcp-devtools",
		"build:az": "bash scripts/mkosi_build azure",
		"build:az:clean": "bash scripts/mkosi_build --clean azure",
		"build:az:devtools": "bash scripts/mkosi_build azure-devtools",
		"build:az:devtools:clean": "bash scripts/mkosi_build --clean azure-devtools",
		"build:azsgx": "bash scripts/mkosi_build azsgx",
		"build:azsgx:clean": "bash scripts/mkosi_build --clean azsgx",
		"build:azsgx:devtools": "bash scripts/mkosi_build azsgx-devtools",
		"build:azsgx:devtools:clean": "bash scripts/mkosi_build --clean azsgx-devtools",
		"clean": "rm -rf build/* kettle-artifacts/* mkosi.cache/* mkosi.builddir/*",
		"clean:az": "bash scripts/cleanup_azure.sh",
		"clean:gcp": "bash scripts/cleanup_gcp.sh",
		"clean:lima": "ssh -F \"$HOME/.lima/tee-builder/ssh.config\" \"lima-tee-builder\" -t -o LogLevel=QUIET -- \"/home/debian/.nix-profile/bin/nix-collect-garbage -d && /home/debian/.nix-profile/bin/nix store optimise && echo 'Nix cache cleaned successfully'\"",
		"deploy:az": "bash scripts/deploy_azure.sh build/kettle-vm-azure.vhd",
		"deploy:az:devtools": "bash scripts/deploy_azure.sh build/kettle-vm-azure-devtools.vhd",
		"deploy:gcp": "bash scripts/deploy_gcp.sh build/kettle-vm-gcp.tar.gz",
		"deploy:gcp:devtools": "bash scripts/deploy_gcp.sh build/kettle-vm-gcp-devtools.tar.gz",
		"deploy:azsgx": "bash scripts/deploy_azure.sh build/kettle-vm-azsgx.vhd",
		"deploy:azsgx:devtools": "bash scripts/deploy_azure.sh build/kettle-vm-azsgx-devtools.vhd",
		"redeploy:az": "bash scripts/redeploy_azure.sh build/kettle-vm-azure.vhd",
		"redeploy:az:devtools": "bash scripts/redeploy_azure.sh build/kettle-vm-azure-devtools.vhd",
		"redeploy:gcp": "bash scripts/redeploy_gcp.sh build/kettle-vm-gcp.tar.gz",
		"redeploy:gcp:devtools": "bash scripts/redeploy_gcp.sh build/kettle-vm-gcp-devtools.tar.gz",
		"redeploy:azsgx": "bash scripts/redeploy_azure.sh build/kettle-vm-azsgx.vhd",
		"redeploy:azsgx:devtools": "bash scripts/redeploy_azure.sh build/kettle-vm-azsgx-devtools.vhd",
		"ls:az": "bash scripts/ls_azure.sh",
		"ls:az:groups": "bash scripts/ls_azure_groups.sh",
		"ls:gcp": "bash scripts/ls_gcp.sh",
		"config:az": "bash scripts/config_azure.sh",
		"config:azsgx": "bash scripts/config_azure.sh",
		"config:gcp": "bash scripts/config_gcp.sh",
		"reset:lima": "limactl stop tee-builder && limactl delete tee-builder",
		"update:genkeys_secureboot": "bash scripts/genkeys_secureboot.sh",
		"update:gpgkeys": "bash scripts/update_gpg_keys.sh",
		"update:gpgkeys:show": "bash scripts/update_gpg_keys.sh show"
	}
}

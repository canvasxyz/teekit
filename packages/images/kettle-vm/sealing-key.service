[Unit]
Description=SEV-SNP Sealing Key Derivation
After=network-setup.service
Before=cloud-launcher.service kettle-launcher.service

[Service]
Type=oneshot
User=root
Group=root
ExecStart=/usr/bin/sealing-key
RemainAfterExit=yes
StandardOutput=append:/var/log/sealing-key.log
StandardError=append:/var/log/sealing-key.log

[Install]
WantedBy=minimal.target
